{
  "name": "URL Blocker",
  "version": "0.1",
  "manifest_version": 3,
  "description": "Uses the chrome.declarativeNetRequest API to block requests.",
  "permissions": ["declarativeNetRequest", "declarativeNetRequestFeedback", "sidePanel"],
  "side_panel": {
    "default_path": "sidepanel.html",
    "type": "module"
  }
}
